        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="400" d:DesignHeight="200"
        x:Class="lab5.Views.RegexWindow"
        Width="400"
        Height="200"
        Icon="/Assets/avalonia-logo.ico"
        Title="RegexWindow">
  
